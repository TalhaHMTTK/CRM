<div class="container mt-5">
        <h1 class="display-4 mb-4 text-center">All Records of <%= @name %> Logins</h1>

        <% @contact.each do |contact| %> 
        <div class="record-card">
            <h4>Email: <span class="text-primary"><%= contact.email %></span></h4> 
            <h4>Logged In on: <span class="text-secondary"><%= contact.created_at.strftime("%B %d, %Y %H:%M") %></span></h4>
        </div>
        <% end %> 
</div>